<h1>Listing users</h1>

<%=
  link_to 'Add User', routes.new_user_path
%>

<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Email</td>
    </tr>
  </thead>

  <tbody>
    <% users.each do |u| %>
      <tr>
        <td><%= u.username %></td>
        <td><%= u.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
